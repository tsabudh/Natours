{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Express Node MongoDb",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js --ext ",
    "dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production & nodemon server.js",
    "debug:dev": " ndb SET NODE_ENV=development server.js",
    "debug:prod": " ndb SET NODE_ENV=production server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js  ",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js  ",
    "build": "netlify deploy --prod",
    "netlify": "netlify build && netlify deploy --prod",
    "watch:sass": "sass --watch scss/main.scss public/css/style.css"
  },
  "author": "Sabudh Bahadur Thapa",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.5.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^8.5.1",
    "libvips": "^0.0.2",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "netlify-cli": "^15.4.1",
    "netlify-lambda": "^2.0.16",
    "nodemailer": "^6.7.8",
    "pug": "^3.0.2",
    "serverless-http": "^3.2.0",
    "sharp": "^0.32.1",
    "slugify": "^1.6.5",
    "stripe": "^12.9.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.7.1",
    "sass": "^1.63.4"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
